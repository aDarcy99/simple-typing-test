{
  "name": "typing-test-node",
  "version": "1.0.0",
  "description": "A simple typing test",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && node server/app.js",
    "client": "webpack serve --mode development --hot",
    "server": "nodemon server/app.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-prebuild": "npm install -f",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "express": "^4.18.2",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "autoprefixer": "^10.3.1",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "concurrently": "^6.2.0",
    "css-loader": "^6.0.0",
    "nodemon": "^2.0.12",
    "postcss": "^8.3.5",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^6.7.0",
    "style-loader": "^3.1.0",
    "tailwindcss": "^2.2.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "author": "Lachlan D'Arcy",
  "license": "ISC"
}
